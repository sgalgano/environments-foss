#!/bin/bash -

if [ -n "$ENVIRONMENTS_EMANE_NODE_VIEW_GUARD" ]
then
    echo "Blocked by EMANE Node View Guard"
    return
fi

ENVIRONMENTS_EMANE_NODE_VIEW_GUARD=1

ENV_ROOT=${ENV_ROOT:=@ENV_ROOT@}

. $ENV_ROOT/common.env
. $ENV_ROOT/emane.env
. $ENV_ROOT/otestpoint.env

echo 'sourcing environment EMANE Node View...'

#
# emane-node-view
#
EMANE_NODE_VIEW_ROOT=${EMANE_NODE_VIEW_ROOT:=$DEV_ROOT/emane-node-view}

export PYTHONPATH=$PYTHONPATH:\
$EMANE_NODE_VIEW_ROOT/build/lib:\
$EMANE_NODE_VIEW_ROOT/build/lib.linux-$ARCH-$PYTHONVERSION

export PATH=\
$EMANE_NODE_VIEW_ROOT/build/scripts-$PYTHONVERSION:\
$PATH
